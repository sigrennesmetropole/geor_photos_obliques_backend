{
	"openapi": "3.0.0",
	"info": {
		"description": "API Photos Obliques",
		"version": "1.0.0",
		"title": "photos-obliques-api",
		"termsOfService": "http://swagger.io/terms/",
		"contact": {
			"email": "thomas.brule@open-groupe.com"
		},
		"license": {
			"name": "Apache 2.0",
			"url": "http://www.apache.org/licenses/LICENSE-2.0.html"
		}
	},
	"paths": {},
	"components": {
		"schemas": {
			"ApiError": {
				"type": "object",
				"properties": {
					"code": {
						"type": "string"
					},
					"label": {
						"type": "string"
					}
				}
			},
			"ApplicationConfiguration": {
				"type": "object",
				"properties": {
					"version": {
						"type": "string",
						"description": "Numéro de version de l'application"
					},
					"comment": {
						"type": "string",
						"description": "Description de la version de l'application"
					},
					"serverPort": {
						"type": "string",
						"description": "Port sur lequel le serveur d'applications est en cours d'exécution"
					},
					"toleranceAngle": {
						"type": "string",
						"description": "Tolérance (en degré) d'acceptation de photo selon leur angle de prise de vue"
					},
					"urlVignette": {
						"type": "string",
						"description": "Url du point de montage public d'accès aux vignettes des photos"
					},
					"urlApercu": {
						"type": "string",
						"description": "Url du point de montage public d'accès aux aperçus des photos"
					},
					"accesPhotosHD": {
						"type": "string",
						"description": "Espace de stockage contenant les fichiers sources des photos (HD)"
					},
					"tailleMaxPanier": {
						"type": "integer",
						"format": "int4",
						"description": "Nombre maximal de photo que l'on peut télécharger"
					}
				}
			},
			"PageResult": {
				"type": "object",
				"properties": {
					"total": {
						"type": "integer",
						"format": "int64"
					}
				}
			},
			"User": {
				"description": "Utilisateur",
				"type": "object",
				"properties": {
					"uuid": {
						"type": "string",
						"format": "uuid"
					},
					"login": {
						"type": "string",
						"description": "Login de l'utilisateur"
					},
					"lastname": {
						"type": "string",
						"description": "Nom de l'utilisateur"
					},
					"firstname": {
						"type": "string",
						"description": "Prénom de l'utilisateur"
					},
					"email": {
						"type": "string"
					},
					"restricted": {
						"type": "boolean"
					},
					"roles": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"organization": {
						"type": "string",
						"description": "Nom de l'organisation de l'utilisateur"
					}
				}
			},
			"PhotosObliquesPageResult": {
				"allOf": [
					{
						"$ref": "#/components/schemas/PageResult"
					},
					{
						"type": "object",
						"properties": {
							"elements": {
								"type": "array",
								"items": {
									"$ref": "#/components/schemas/PhotoOblique"
								}
							}
						}
					}
				]
			},
			"PhotoOblique": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "fichier": {
                        "type": "string"
                    },
                    "annee": {
                        "type": "integer",
                        "format": "int4"
                    },
                    "date": {
                        "type": "string"
                    },
                    "heure": {
                       "type": "string"
                    },
                    "angleDegre": {
						"type": "number",
						"format": "double",
						"multipleOf": 0.001
                    },
                    "angleGradient": {
						"type": "number",
						"format": "double",
						"multipleOf": 0.001
                    },
                    "presta": {
                        "type": "string"
                    },
                    "proprio": {
                        "type": "string"
                    },
                    "mention": {
                       "type": "string"
                    },
                    "commune": {
                        "type": "string"
                    },
                    "shape": {
                       "type": "string"
                    },
                    "tailleFichier": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "pertinence": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "urlVignette": {
                        "type": "string"
                    },
                    "urlApercu": {
                        "type": "string"
                    }
                }
			},
			"PhotosCount": {
				"type": "object",
				"properties": {
					"numberOfResult": {
						"type": "integer",
						"format": "int32"
					}
				}
			},
			"AvailableYears": {
				"type": "object",
				"properties": {
					"availableDates": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				}
			},
			"AvailableProviders": {
				"type": "object",
				"properties": {
					"availableDates": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				}
			},
			"AvailableOwners": {
				"type": "object",
				"properties": {
					"availableDates": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				}
			}
		}
	}
}
