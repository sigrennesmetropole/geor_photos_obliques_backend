						"in": "query",
						"name": "endDate",
						"required": false,
						"schema": {
							"type": "integer",
							"format": "int4",
							"description": "Date de fin de la recherche"
						}
					},
					{
						"in": "query",
						"name": "angleDegree",
						"description": "Index de début",
						"required": false,
						"schema": {
							"type": "number",
							"format": "double",
							"multipleOf": 0.001
						}
					},
					{
						"in": "query",
						"name": "provider",
						"description": "todo",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "owner",
						"required": false,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/PhotosCount"
								}
							}
						}
					},
					"400": {
						"description": "Paramètre obligatoire manquant",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/ApiError"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/ApiError"
								}
							}
						}
					}
				}
			}
		},
		"/photos/download": {
			"get": {
				"tags": [
					"photos"
				],
				"summary": "Download photos",
				"description": "Génère et renvoie un zip contenant les photos et les informations correspondantes",
				"operationId": "downloadPhotos",
				"parameters": [
					{
						"in": "query",
						"name": "photoIds",
						"description": "Les photos à télécharger",
						"required": true,
						"schema": {
							"type": "array",
							"items": {
								"type": "string"
							}
						}
					},
					{
						"in": "query",
						"name": "zipName",
						"description": "nom du zip généré",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "prefix",
						"description": "prefix du zip généré",
						"required": false,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/octet-stream": {
								"schema": {
									"type": "string",
									"format": "binary"
								}
							}
						}
					},
					"400": {
						"description": "Paramètre obligatoire manquant ou ressource introuvable",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/ApiError"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/ApiError"
								}
							}
						}
					}
				}
			}
		},
