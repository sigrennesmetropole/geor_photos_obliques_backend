		"/photos": {
			"get":{
				"tags": [
					"photos"
				],
				"summary": "Recherche photos. Attention, les champs null en base de données ne sont pas retournés.",
				"description": "Recherche des photos. Attention, les champs null en base de données ne sont pas retournés.",
				"operationId": "searchPhotos",
				"parameters": [
					{
						"in": "query",
						"name": "geometry",
						"required": true,
						"schema": {
							"type": "string",
							"description": "Géométrie WKT de l'emprise de recherche"
						}
					},
					{
						"in": "query",
						"name": "startDate",
						"required": false,
						"schema": {
							"type": "integer",
							"format": "int4",
							"description": "Date de début de la recherche"
						}
					},
					{
						"in": "query",
						"name": "endDate",
						"required": false,
						"schema": {
							"type": "integer",
							"format": "int4",
							"description": "Date de fin de la recherche"
						}
					},
					{
						"in": "query",
						"name": "angleDegree",
						"description": "Index de début",
						"required": false,
						"schema": {
							"type": "number",
							"format": "double",
							"multipleOf": 0.001
						}
					},
					{
						"in": "query",
						"name": "provider",
						"description": "todo",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "owner",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "offset",
						"description": "Index de début",
						"required": false,
						"schema": {
							"type": "integer",
							"format": "int32"
						}
					},
					{
						"in": "query",
						"name": "limit",
						"description": "Nombre de résultats à retourner",
						"required": false,
						"schema": {
							"type": "integer",
							"format": "int32"
						}
					},
					{
						"in": "query",
						"name": "order",
						"required": false,
						"schema": {
							"type": "string",
							"description": "liste des tris à réaliser. Exemple :  code,-libelle"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/PhotosObliquesResult"
								}
							}
						}
					},
					"400": {
						"description": "Paramètre obligatoire manquant",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/ApiError"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "photos-obliques-model.json#/components/schemas/ApiError"
								}
							}
						}
					}
				}
			}
		},
		"/photos/count": {
			"get":{
				"tags": [
					"photos"
				],
				"summary": "Count photos",
				"description": "Compte le nombre de photos correspondant à une recherche",
				"operationId": "countPhotos",
				"parameters": [
					{
						"in": "query",
						"name": "geometry",
						"required": true,
						"schema": {
							"type": "string",
							"description": "Géométrie WKT de l'emprise de recherche"
						}
					},
					{
						"in": "query",
						"name": "startDate",
						"required": false,
